{
  "name": "mempatch",
  "version": "1.0.1",
  "description": "Runtime memory patching for Node.js/Electron",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "install": "node-gyp-build",
    "prebuild": "prebuildify --napi --strip",
    "build": "prebuildify --napi --strip"
  },
  "binary": {
    "module_name": "mempatch",
    "module_path": "prebuilds",
    "host": "https://github.com/SammCheese/Mempatch/releases/download",
    "napi_versions": [
      8
    ]
  },
  "files": [
    "prebuilds/",
    "index.js",
    "index.d.ts",
    "build/",
    "src/"
  ],
  "keywords": [
    "memory",
    "patch",
    "signature",
    "bytecode",
    "reverse-engineering"
  ],
  "author": "SammCheese <sammy@sammcheese.net>",
  "license": "MIT",
  "packageManager": "pnpm@10.12.2",
  "engines": {
    "node": ">=16.0.0"
  },
  "gypfile": true,
  "dependencies": {
    "node-addon-api": "^8.4.0",
    "node-gyp-build": "^4.8.4"
  },
  "devDependencies": {
    "node-gyp": "^11.2.0",
    "prebuildify": "^6.0.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SammCheese/Mempatch.git"
  }
}
